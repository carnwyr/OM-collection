          var selectedType = $("input[name='spgRadio2']:checked").val();

          $("#myTable>tr").filter(function() {
            $(this).toggle($(this).text().includes(selectedChar+selectedType));
            if (selectedType === "dancebattle") {
              $("#dancebattle", this).attr("rowspan", 6);
              $("#homescreen", this).hide();
